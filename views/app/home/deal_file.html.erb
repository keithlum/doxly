    <!-- Modals -->
    <div class="modal fade" id="modal-edit-deal" tabindex="-1" role="dialog" aria-labelledby="modal-edit-deal-label">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title" id="modal-edit-deal-label">Edit Deal</h4>
                </div>
                <div class="modal-body">
                    <form>
                        <div class="container-fluid">
                            <div class="row">
                                <div class="col-xs-12 col-md-6">
                                    <div class="form-group">
                                        <label for="input-deal-title">Deal Title</label>
                                        <input value="Project Panda" type="text" name="deal_title" id="input-deal-title" class="form-control" placeholder="Give your task a title" required="">
                                    </div>
                                    <div class="form-group">
                                        <label for="input-deal-client">Client Name</label>
                                        <input value="Panda Express" type="text" name="deal_client" id="input-deal-client" class="form-control" placeholder="Add your client's name or other identifier" required="">
                                    </div>
                                    <div class="form-group">
                                        <label for="input-deal-date">Projected Close Date</label>
                                        <input value="07/24/16" type="text" name="deal_date" id="input-deal-date" class="form-control" placeholder="MM/DD/YYYY" required="">
                                    </div>
                                    <div class="form-group">
                                        <label for="input-deal-transaction">Transaction Type</label>
                                        <select id="input-deal-transaction" class="selectpicker form-control show-tick">
                                            <option selected="selected">Merger</option>
                                            <option>Type 2</option>
                                            <option>Type 3</option>
                                        </select>

                                    </div>
                                    <div class="form-group">
                                        <label for="input-deal-size">Deal Size</label>
                                        <input value="$1,500,000,000" type="text" name="deal_size" id="input-deal-size" class="form-control" placeholder="$" required="">
                                    </div>
                                </div>
                                <div class="col-xs-12 col-md-6">
                                    <div class="form-group">
                                        <label for="input-deal-collaborators">Collaborators</label>
                                        <input type="text" name="deal_collaborators_input" id="input-deal-collaborators" class="form-control" placeholder="Enter a name or email address"/>
                                        <div class="deal-collaborators-widget">
                                            <ul>
                                                <li data-collaborator-id="1">
                                                    <a href="#">
                                                        <%= image_tag "img-avatar-2.png", class: "avatar" %>
                                                    </a>
                                                </li>
                                                <li data-collaborator-id="5">
                                                    <a href="#">
                                                        <%= image_tag "img-avatar-3.png", class: "avatar" %> Harvey Specter
                                                    </a>
                                                </li>
                                                <li data-collaborator-id="9">
                                                    <a href="#">
                                                        <%= image_tag "img-avatar-4.png", class: "avatar" %> Rebecca Moss
                                                    </a>
                                                </li>
                                                <li data-collaborator-id="12">
                                                    <a href="#">
                                                        <%= image_tag "img-avatar-5.png", class: "avatar" %> Louisa Curtis
                                                    </a>
                                                </li>
                                                <li data-collaborator-id="18">
                                                    <a href="#">
                                                        <%= image_tag "img-avatar-6.png", class: "avatar" %> Cecilia Hopkins
                                                    </a>
                                                </li>
                                                <li data-collaborator-id="92">
                                                    <a href="#">
                                                        <%= image_tag "img-avatar-1.png", class: "avatar" %> Alvin Jones
                                                    </a>
                                                </li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
                <div class="modal-footer">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-xs-12">
                                <button type="button" class="btn btn-default pull-left" data-dismiss="modal">Cancel</button>
                                <button type="button" class="btn btn-primary">Create</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="modal fade" id="modal-new-section" tabindex="-1" role="dialog" aria-labelledby="modal-new-section-label">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title" id="modal-new-section-label">New Section</h4>
                </div>
                <div class="modal-body">
                    <form>
                        <div class="form-group">
                            <label for="input-section-title">Section Title</label>
                            <input type="text" name="deal_title" id="input-section-title" class="form-control" placeholder="Give your section a title" required="">
                        </div>
                    </form>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default pull-left" data-dismiss="modal">Cancel</button>
                    <button type="button" class="btn btn-primary">Create</button>
                </div>
            </div>
        </div>
    </div>

    <div class="modal fade" id="modal-new-task" tabindex="-1" role="dialog" aria-labelledby="modal-new-task-label">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title" id="modal-new-task-label">New Task</h4>
                </div>
                <div class="modal-body">
                    <form>
                        <div class="form-group">
                            <label for="input-task-title">Task Title</label>
                            <input type="text" name="task_title" id="input-task-title" class="form-control" placeholder="Give your task a title" required="">
                        </div>
                        <div class="form-group optional">
                            <label for="input-task-desc">Task Description</label>
                            <textarea rows="5" name="task_desc" id="input-task-desc" class="form-control" placeholder="Explain this task" ></textarea>
                        </div>

                        <div class="form-group optional">
                            <label for="input-task-section">Add to Section</label>
                            <select name="task_section" id="input-task-section" class="selectpicker form-control show-tick">
                                <option>Select a Section</option>
                                <option>Section 1</option>
                                <option>Section 2</option>
                                <option>Section 3</option>
                            </select>
                        </div>

                        <div class="form-group optional">
                            <label for="input-task-assignee">Add an Assignee</label>
                            <select name="task_assignee" id="input-task-assignee" class="selectpicker form-control show-tick">
                                <option>Select a Team Member</option>
                                <option>Team Member 1</option>
                                <option>Team Member 2</option>
                                <option>Team Member 3</option>
                            </select>
                        </div>
                    </form>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default pull-left" data-dismiss="modal">Cancel</button>
                    <button type="button" class="btn btn-primary">Create</button>
                </div>
            </div>
        </div>
    </div>


    <div class="modal fade" id="modal-new-file" tabindex="-1" role="dialog" aria-labelledby="modal-new-file-label">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title" id="modal-new-file-label">New Section</h4>
                </div>
                <div class="modal-body">
                    <form>
                        <div class="form-group">
                            <label for="input-section-title">Upload</label>
                            <input class="file-inputs" type="file" name="file_name" id="input-file-title" title="<i class='icon-icon-uploadcloud'></i> Choose a File" data-filename-placement="inside">
                        </div>

                        <div class="form-group optional">
                            <label for="input-file-section">Add to Section</label>
                            <select name="file_section" id="input-file-section" class="selectpicker form-control show-tick">
                                <option>Select a Section</option>
                                <option>Section 1</option>
                                <option>Section 2</option>
                                <option>Section 3</option>
                            </select>
                        </div>
                    </form>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default pull-left" data-dismiss="modal">Cancel</button>
                    <button type="button" class="btn btn-primary">Create</button>
                </div>
            </div>
        </div>
    </div>


    <div class="modal fade" id="modal-new-folder" tabindex="-1" role="dialog" aria-labelledby="modal-new-folder-label">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
